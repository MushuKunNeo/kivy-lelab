#: import layout_exemples layout_exemples
#: import widget_exemples widget_exemples
#: import navigation_screen_manager navigation_screen_manager
#: import BoxLayout_with_action_bar BoxLayout_with_action_bar
#: import CanvasExempleTabs canvas_exemples

<MenuPrincipal@BoxLayout>
    orientation: "vertical"
    Button:
        text: "Layout"
        on_press:  app.manager.push("LayoutExemplesTabs")
    Button:
        text: "Widgets"
        on_press:  app.manager.push("WidgetsExemple")
    Button:
        text: "Images"
        on_press:  app.manager.push("ImageExemple")
    Button:
        text: "Canvas"
        on_press:  app.manager.push("CanvasExempleTabs")

<MyScreenManager@NavigationScreenManager>:
    Screen:
        name:"MenuPrincipal"
        MenuPrincipal:
    Screen:
        name: "LayoutExemplesTabs"
        BoxLayoutWithActionBar:
            title: "Layout"
            LayoutExemplesTabs:
    Screen:
        name: "WidgetsExemple"
        BoxLayoutWithActionBar:
            title: "Widgets"
            WidgetsExemple:
    Screen:
        name: "ImageExemple"
        BoxLayoutWithActionBar:
            title: "Images"
            ImageExemple:
    Screen:
        name: "CanvasExempleTabs"
        BoxLayoutWithActionBar:
            title: "Canvas"
            CanvasExempleTabs: